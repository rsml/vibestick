<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.rossthedev.vibestick</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/Vibestick.app/Contents/MacOS/Vibestick</string>
    </array>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <false/>
    
    <key>StandardErrorPath</key>
    <string>/tmp/vibestick.err</string>
    
    <key>StandardOutPath</key>
    <string>/tmp/vibestick.out</string>
</dict>
</plist>